        <owl:Restriction>
          <owl:onProperty rdf:resource="#has_A_Voltage"/>
          <owl:allValuesFrom rdf:resource="#Postive_(+)"/>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <owl:Restriction>
          <owl:onProperty rdf:resource="#has_A_Configuration"/>
          <owl:allValuesFrom rdf:resource="#P-channel_Configuration"/>
        </owl:Restriction>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#has_A_Voltage"/>
          <owl:allValuesFrom rdf:resource="#Negative_(-)"/>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
  <rdfs:subClassOf rdf:resource="#Property"/>
