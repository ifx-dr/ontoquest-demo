        <rdfs:range rdf:resource="http://www.w3.org/ns/ssn/System"/>
        <rdfs:comment xml:lang="en">Relation between a Deployment and a deployed System.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/ssn/"/>
