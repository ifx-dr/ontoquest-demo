    <!-- http://www.w3.org/TR/vocab-ssn-ext/hasMember -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/TR/vocab-ssn-ext/hasMember">
        <rdfs:domain rdf:resource="http://www.w3.org/TR/vocab-ssn-ext/ObservationCollection"/>
