        <rdfs:label>nature of relationship</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/sosa/sampling/relatedSample -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/sosa/sampling/relatedSample">
