    <!-- http://www.w3.org/ns/ssn/deployedOnPlatform -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/deployedOnPlatform">
        <owl:inverseOf rdf:resource="http://www.w3.org/ns/ssn/inDeployment"/>
