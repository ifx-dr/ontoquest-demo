<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology"
     xml:base="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:sc3="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#sc3:"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:time="http://www.w3.org/2006/time#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:Planning-Ontology="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology">
        <owl:imports rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/ozone-ontology"/>
        <owl:imports rdf:resource="http://www.w3.org/2006/time#2016"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#sc3:formula -->

    <owl:AnnotationProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#sc3:formula"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2001/XMLSchema#duration -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#duration"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasActivities -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasActivities">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningObject"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#RequiredCapacity"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/ozone-ontology#Product"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Activity"/>
        <rdfs:comment>This property describes the set of activities that must be carried out to produce the goods that are associated with the PLANNING OBJECT.

Moreover, there are additional parameters for most types of PLANNING OBJECTS.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasBillOfMaterial -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasBillOfMaterial">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Part"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/ozone-ontology#Product"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#BillOfMaterial"/>
        <rdfs:comment>A set of potentially alternative BOMs can be used to meet the demand of a certain product</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasBin -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasBin">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Binning"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Bin"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasCapacity -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasCapacity">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Process"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/ozone-ontology#Resource"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#AvailableCapacity"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasChildProductAggregate -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasChildProductAggregate">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#ProductHierarchy"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#ProductHierarchy"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasComponentLocation -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasComponentLocation">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#BillOfMaterial"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Location"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasComponentPart -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasComponentPart">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#BillOfMaterial"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Process"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#ComponentPart"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasComponentProcess -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasComponentProcess">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#BillOfMaterial"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Process"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasConnection -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasConnection">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#DecisionMakingUnit"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningProcess"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Edge"/>
        <rdfs:comment>If a vertex provides instruction or a reaction to another vertex than the two vertices are connected by a directed edge.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasConstraint -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasConstraint"/>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasCriterias -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasCriterias">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlaningPurpose"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Criteria"/>
        <rdfs:comment>A criterion is assigned to each performance measure that belongs to a PLANNING PURPOSE. Typical criteria are maximization, minimization, i.e. if an optimization problem is considered, or larger or smaller than a prescribed value of the certain performance measure, a so-called target, i.e. if a satisfaction problem is solved. In a multi-criteria setting, the dominance concept for plans is of interest.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasDMUa -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasDMUa">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Vertex"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#DecisionMakingUnit"/>
        <rdfs:comment>First DMU of the Vertex</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasDMUb -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasDMUb">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Edge"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#DecisionMakingUnit"/>
        <rdfs:comment>Second DMU of the Vertex</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasDecisionMakerType -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasDecisionMakerType">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#DecisionMakingUnit"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#DecisionMakerType"/>
        <rdfs:comment>Specifies the concrete type of a DMU</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasDecisionMakingInitiative -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasDecisionMakingInitiative">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#DecisionMakingUnit"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#DecisionMakingInitiative"/>
        <rdfs:comment>We distinguish fully autonomous DECISION-MAKING UNITS from DECISION-MAKING UNITS where the decision-making activities are triggered by other DECISION-MAKING UNITS. Note that the decision-making initiative can change over time in a situation-dependent manner.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasDecisionMakingUnit -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasDecisionMakingUnit">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningSituation"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Vertex"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#DecisionMakingUnit"/>
        <rdfs:comment>This property describes the set of DECISION-MAKING UNITS that constitutes the planning situation. We refer to the elements of the set as vertices.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasDecisionMakingUnitCharcters -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasDecisionMakingUnitCharcters">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningSituation"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#DecisonMakingUnitCharacter"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasDefined -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasDefined">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanExecutuon"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PerformanceMeasure"/>
        <rdfs:comment>Values for certain PERFORMANCE MEASURES can be computed after a plan is executed. This always requires that the consequences of a PLAN are assessed in the base system.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasEarliestReleaseDate -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasEarliestReleaseDate">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningObject"/>
        <rdfs:range rdf:resource="http://www.w3.org/2006/time#TemporalDuration"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasEventTrigger -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasEventTrigger">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningProcess"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Trigger"/>
        <rdfs:comment>This property determines which events result in replanning (Vieria et al. 2003). For instance, replanning can be the result of the breakdown of a bottleneck resource.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasFrozenFence -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasFrozenFence">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningProcess"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningDecision"/>
        <rdfs:comment>This property specifies which planning decisions are remain unchanged in the case of replanning activities.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasHierarchicalSituation -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasHierarchicalSituation">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningSituation"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#HierarchicalSituation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasInvolvedPlanningObjects -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasInvolvedPlanningObjects">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningInstance"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningObject"/>
        <rdfs:comment>This property specifies the involved PLANNING OBJECTS.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasLocation -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasLocation">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#BillOfMaterial"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Binning"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Capacity"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#FrozenFence"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Inventory"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#InventoryPolicy"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#LotSizingRule"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningObject"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Process"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#ProjectedWip"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Substitution"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#SupplyInformation"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Yield"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/ozone-ontology#Product"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Location"/>
        <rdfs:comment>This property describes the place or places, for instance nodes of the semiconductor supply chain or specific machines of a work center where the ACTIVITIES associated with the PLANNING OBJECT have to be executed.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasObjectives -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasObjectives">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningProcess"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlaningPurpose"/>
        <rdfs:comment>: The property specifies the objectives of a certain planning activity.
Moreover, there are additional parameters for most types of PLANNING PROCESS.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasOperations -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasOperations">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Route"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#MacroOperation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasOrder -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasOrder">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#In-TransitInventory"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Order"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasOwner -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasOwner">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningObject"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Owner"/>
        <rdfs:comment>A PLANNING OBJECT is rarely considered in isolation. Usually it belongs to a set of PLANNING OBJECTS, for example, a planning instance.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasParentProductAggregate -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasParentProductAggregate">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#ProductHierarchy"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#ProductHierarchy"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasPart -->

