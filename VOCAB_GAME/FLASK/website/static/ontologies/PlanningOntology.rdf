        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>This property specifies which maximum time is allowed to perform a certain planning activity.</rdfs:comment>
    </owl:DatatypeProperty>
    


