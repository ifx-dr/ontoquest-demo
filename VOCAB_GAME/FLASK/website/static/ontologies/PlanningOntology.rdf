    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasQuantity">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningObject"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/ozone-ontology#Demand"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:comment>This property describes given, i.e. fixed, size of a PLANNING OBJECT, for instance, the number of wafers that belong to a lot. Note that the parameter QUANTITY is exogenous, i.e. given, in contrast to the property QUANTITY where the amount of the quantity has to be decided.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasReleaseDate -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasReleaseDate">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Activity"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningObject"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasRequestedDate -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasRequestedDate">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/ozone-ontology#Demand"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasRevenue -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasRevenue">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlaningPurpose"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Revenue refers to the amount of income, earning generated by the sale of one unit of a certain good.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasSafetyStockLevel -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasSafetyStockLevel">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#InventoryPolicy"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasSafetyStockTime -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasSafetyStockTime">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#InventoryPolicy"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasSetOfConnections -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasSetOfConnections">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningSituation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasSoftConstraints -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasSoftConstraints">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningConstraint"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>PLANNING CONSTRAINTS with this property can be violated. This means that the degree of violation is typically penalized in a related criterion or performance measure.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasStartDate -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasStartDate">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Binning"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#InventoryPolicy"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#LotSizingRule"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Substitution"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasStartTime -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasStartTime">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningConstraint"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>This is the earliest point in time when the PLANNING CONSTRAINT is active.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasSubstitutionQuantity -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasSubstitutionQuantity">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Substitution"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasTime -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasTime">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningObject"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>This property is required to manage timing aspects of a PLANNING OBJECT. It can be a release time, a finishing time, a single start time, or a set of start time for certain ACTIVITIES to be fulfilled.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasType -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasType">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#InventoryPolicy"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Order"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#SupplyInformation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasWeights -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasWeights">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlaningPurpose"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:comment>Weights can be used to express preferences of the decision makers among the different performance measures and related criteria. This is, for instance, useful if a lexicographic approach is used.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#numberOfOperationsOnSameResource -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#numberOfOperationsOnSameResource">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/ozone-ontology#Product"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2002/07/owl#topDataProperty -->

    <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#topDataProperty">
        <rdfs:comment>This is the earliest time where the activities associated with a PLANNING OBJECT can start. Note that the parameter RELEASE DATE is exogenous, i.e. given, in contrast to the property TIME.</rdfs:comment>
    </rdf:Description>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Activity -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Activity"/>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Automated -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Automated">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#DecisionMakerType"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Autonomous -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Autonomous">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#DecisionMakingInitiative"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#AvailableCapacity -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#AvailableCapacity">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Capacity"/>
        <rdfs:comment>: The AVAILABLE CAPACITY describes the ability of a resource within a period of time to perform activities or processes. Elmaghraby (2011) refers to this as operational capacity, i.e. the productive capacity that is obtained from the nominal capacity by subtracting the anticipated and unavoidable loss in productivity, for instance, due to machine breakdowns in a wafer fab.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#BillOfMaterial -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#BillOfMaterial">
        <rdfs:comment>A BILL OF MATERIALS (BOM) describes the component products used to make another product. Substitution, often associated with binning, is based on the possibility that alternative items might be used to finish a process step.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Bin -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Bin"/>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Binning -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Binning">
        <rdfs:comment>BINNING refers to the process of testing and categorizing parts against defined measures and sort them into different bins (Degbotse 2013). Each bin belongs to certain levels of defined measures (e.g. chip speed, current, voltage, energy consumption). Binning is often combined with SUBSTITUTION.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Capacity -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Capacity"/>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#ComponentPart -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#ComponentPart">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Part"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Criteria -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Criteria">
        <rdfs:comment>Typical criteria are maximization, minimization, i.e. if an optimization problem is considered, or larger or smaller than a prescribed value of the certain performance measure, a so-called target, i.e. if a satisfaction problem is solved. In a multi-criteria setting, the dominance concept for plans is of interest.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#DecisionMakerType -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#DecisionMakerType">
        <rdfs:comment>We distinguish human decision-makers (human DECISION-MAKING UNITS) from fully automated DECISION-MAKING UNITS where computers make fully automated decisions. Between these two extremes there are semi-automated decisions, where a computer provides several decision alternatives and a human decision-maker chooses among them. Note that the decision-maker type can change over time.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#DecisionMakingInitiative -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#DecisionMakingInitiative">
        <rdfs:comment>We distinguish fully autonomous DECISION-MAKING UNITS from DECISION-MAKING UNITS where the decision-making activities are triggered by other DECISION-MAKING UNITS. Note that the decision-making initiative can change over time in a situation-dependent manner.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#DecisionMakingUnit -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#DecisionMakingUnit">
        <rdfs:comment>The DECISION-MAKING UNIT specifies an instance that makes decisions.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#DecisonMakingUnitCharacter -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#DecisonMakingUnitCharacter">
        <rdfs:comment>In an ANTAGONISTIC SETTING, the different DECISION-MAKING UNITS try to meet their own objectives which are in conflict. In a non-antagonistic setting, the DECISION-MAKING UNITS do not exploit their private knowledge in an opportunistic way.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Dependent -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Dependent">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#DecisionMakingInitiative"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#DistributedHierarchicalSituation -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#DistributedHierarchicalSituation">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#HierarchicalSituation"/>
        <rdfs:comment>In this situation, edges referring to instructions and reactions are possible.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Edge -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Edge"/>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#FrozenFence -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#FrozenFence">
        <rdfs:comment>FROZEN FENCE is used to set a frozen interval  in which decision making is not allowed to change the previously determined values.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#HeterarchicalSituation -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#HeterarchicalSituation">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#HierarchicalSituation"/>
        <rdfs:comment>All the vertices have level 1.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#HierarchicalSituation -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#HierarchicalSituation"/>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Human -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Human">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#DecisionMakerType"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#In-TransitInventory -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#In-TransitInventory">
        <rdfs:comment>The IN-TRANSIT INVENTORY describes the quantity of a certain product that is shipped from one location to another. This quantity has left the inventory of one location, but has not yet been received into the inventory of the receiving location.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Inventory -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Inventory">
        <rdfs:comment>The INVENTORY describes the quantity of a certain product that is available in stock to be used to meet demand.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#InventoryPolicy -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#InventoryPolicy">
        <rdfs:comment>An INVENTORY POLICY indicates how much earlier or how much more (safety stock) needs to be built than that required to satisfy demand on time. Inventory Policies define the rules which have to be implemented at a specific inventory location such as safety stock or product mix for supporting the production process flow as well as fulfilling customer demand.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Location -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Location">
        <rdfs:comment></rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#LotSizingRule -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#LotSizingRule">
        <rdfs:comment>A LOT SIZING RULE defines a rule to specify the lot size for a specific part. The lot size on the other hand defines the number of pieces belonging to a part. The lot size is the result of applying lot sizing rules.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#MacroOperation -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#MacroOperation"/>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Order -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Order"/>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Owner -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Owner"/>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Part -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Part">
        <rdfs:comment>A PART represents a physical instance of a product in the manufacturing process. It therefore realizes a product. A part moves through the system and is manufactured according to its product specification. A part is associated to exactly one product. During the production process it is typically tested several times. The binning rules of the associated product specification can be applied to the respective part. A part can be substituted by another part according to the associated product specification</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PerformanceMeasure -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PerformanceMeasure">
        <rdfs:comment>Each measure is defined by a calculation procedure. Performance measures can be blended using weights to express the importance of the different measures.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Plan -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Plan">
        <rdfs:comment>A PLAN is an activity specification with an intended purpose.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanExecutuon -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanExecutuon">
        <rdfs:comment>A PLAN EXECUTION is related to performing the activities according to a PLAN either directly in the base system, for instance, releasing lots into a wafer fab, or as instructions for another DECISION-MAKING UNIT.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlaningPurpose -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlaningPurpose">
        <rdfs:comment></rdfs:comment>
        <rdfs:comment>The PLANNING PURPOSE specifies the objectives of a certain planning activity. It is given by a set of performance measures that are related to a set of PLANNING OBJECTS.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningApproach -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningApproach">
        <rdfs:comment>Specific planning algorithm which is used in a deticated planning process</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningConstraint -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningConstraint">
        <rdfs:comment>A PLANNING CONSTRAINT restricts the action space. This space is formed by alternative actions that are a result of the planning decisions (Schneeweis 2003). In the planning domain, PLANNING CONSTRAINTS, often the capacity offered by resources, limit the quantities that are assigned to the PLANNING OBJECTS or impact the temporal properties of PLANNING OBJECTS.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningDecision -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningDecision">
        <rdfs:comment>Decisions determined by a Decision Making Unit</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningFunction -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningFunction">
        <rdfs:comment>Describes the different functions and its properies follwing the supply chain planning matrix.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningInstance -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningInstance">
        <rdfs:comment>A PLANNING INSTANCE specifies the PLANNING OBJECTS and the values of related PLANNING CONSTRAINTS that form the basis for a planning decision.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningObject -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningObject">
        <rdfs:comment>A PLANNING OBJECT specifies what has to be planned as an atomic unit. A single PLANNING OBJECT typically represents a quantity of a certain product or product aggregate to be released or completed within a certain period in a certain node of the semiconductor supply chain. Other PLANNING OBJECTS can be orders or lots. Values are assigned to the attributes of a PLANNING OBJECT as a result of a planning decision.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningProcess -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningProcess">
        <rdfs:comment>The PLANNING PROCESS specifies under which circumstances which (re)planning activities are performed by a DECISION-MAKING UNIT.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningSituation -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningSituation">
        <rdfs:comment>The PLANNING SITUATION describes the organization of several DECISION-MAKING UNITS and also its environment.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Process -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Process">
        <rdfs:comment>A PROCESS specifies a method of making a product at a location. The process is expressed at a coarse level of granularity suitable for use in production planning.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#ProductAggregate -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#ProductAggregate">
        <rdfs:comment>A PRODUCT AGGREGATE combines individual products into a few logical products groups. This results in a reduction of complexity, which enables a better overview.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#ProductHierarchy -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#ProductHierarchy">
        <rdfs:comment>A PRODUCT HIERARCHY characterizes the top-down and bottom-up relationship between different product aggregates. It organizes the product aggregates according to defined criteria. A product aggregate is used to group different products based on certain criteria. The grouping criteria depend on the planning purpose (e.g. controlling, production). It is a certain view on a set of products.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#ProjectedWip -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#ProjectedWip">
        <rdfs:comment>The PROJECTED WIP describes the quantity of a certain product that is anticipated to be received into inventory at a particular location at a particular time.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#ReplanningDegree -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#ReplanningDegree">
        <rdfs:comment>The degree distinguishes replanning from scratch and repair of existing plans (Vieria et al. 2003).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#RequiredCapacity -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#RequiredCapacity">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Capacity"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Resource-Availability-Constraints -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Resource-Availability-Constraints">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningConstraint"/>
        <rdfs:comment>This type of constraints has an impact on the assignment of activities to RESOURCES and to START/END TIMES to ACTIVITIES.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Resource-Compatibility-Constraints -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Resource-Compatibility-Constraints">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningConstraint"/>
        <rdfs:comment>This type of constraints ensures that only allowed activities are performed using a given RESOURCE.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Route -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Route"/>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#SemiAutomated -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#SemiAutomated">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#DecisionMakerType"/>
        <rdfs:comment>Between these two extremes there are semi-automated decisions, where a computer provides several decision alternatives and a human decision-maker chooses among them.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#StrictHierarchicalSituation -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#StrictHierarchicalSituation">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#HierarchicalSituation"/>
        <rdfs:comment>There are only edges between vertices of level k and vertices of level k-1, where k2.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Substitution -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Substitution">
        <rdfs:comment>SUBSTITUTION refers to using alternative parts. Depending on the product, each one could be replaced by using specific alternatives at the same stage of production, e.g. final product, raw material, or semi-finished products (Degbotse 2013) in order to complete a manufacturing step or fulfill a customer order.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#SupplyInformation -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#SupplyInformation">
        <rdfs:comment>SUPPLY INFORMATION provides information of the used and unused supply of a supply chain network.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Target -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Target">
        <rdfs:comment>Targets are prescribed values for the different performance measures.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Temporal-Constraints -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Temporal-Constraints">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningConstraint"/>
        <rdfs:comment>This type of constraints restricts the values of temporal attributes, modeled by the property TIME, that belong to PLANNING OBJECTS. For instance, there might be a minimum amount of time between two consecutive operations of a certain product.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Trigger -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Trigger">
        <rdfs:comment>A mechanism that initiates an action when an event occurs such as reaching a certain time or date or upon receiving some type of input. A trigger generally causes a program routine to be executed.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Vertex -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Vertex"/>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Yield -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Yield">
        <rdfs:comment>YIELD could be the fraction (percentage) of good dies resulting from total dies released.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/ozone-ontology#Demand -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/ozone-ontology#Demand"/>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/ozone-ontology#Product -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/ozone-ontology#Product"/>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/ozone-ontology#Resource -->

    <owl:Class rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/ozone-ontology#Resource"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Activity_Etching -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Activity_Etching">
        <rdf:type rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Activity"/>
        <Planning-Ontology:hasCapacity rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#RequiredCap1"/>
        <Planning-Ontology:hasResources rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#FE_Aligner"/>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Activity_Oxide -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Activity_Oxide">
        <rdf:type rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Activity"/>
        <Planning-Ontology:hasCapacity rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#RequiredCap2"/>
        <Planning-Ontology:hasResources rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#FE_Implanter"/>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Activity_Photo -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Activity_Photo">
        <rdf:type rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Activity"/>
        <Planning-Ontology:hasCapacity rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#RequiredCap3"/>
        <Planning-Ontology:hasResources rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#FE_Stepper_1"/>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Activity_UV_Bake -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Activity_UV_Bake">
        <rdf:type rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Activity"/>
        <Planning-Ontology:hasCapacity rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#RequiredCap4"/>
        <Planning-Ontology:hasResources rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#FE_Stepper_2"/>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#All_Planning_Objects_have_to_be_positive -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#All_Planning_Objects_have_to_be_positive">
        <rdf:type rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningConstraint"/>
        <sc3:formula>forall($g in Products, $t in PlanningHorizon, $j in Frontends) -&gt; FE_Y($j, $g, $t) &gt;= 0;
forall($g in Products, $t in PlanningHorizon, $j in Frontends) -&gt; FE_X($j, $g, $t) &gt;= 0;
forall($g in Products, $t in PlanningHorizon, $j in Frontends) -&gt; FE_Wip($j, $g, $t) &gt;= 0;
forall($g in Products, $t in PlanningHorizon) -&gt; FE_Inventory($g, $t) &gt;= 0;
forall($g in Products, $t in PlanningHorizon) -&gt; FE_Backlog($g, $t) &gt;= 0;</sc3:formula>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#AvailableCap1 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#AvailableCap1">
        <rdf:type rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#AvailableCapacity"/>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#AvailableCap2 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#AvailableCap2">
        <rdf:type rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#AvailableCapacity"/>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#AvailableCap3 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#AvailableCap3">
        <rdf:type rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#AvailableCapacity"/>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#AvailableCap4 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#AvailableCap4">
        <rdf:type rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#AvailableCapacity"/>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#AvailableCap5 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#AvailableCap5">
        <rdf:type rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#AvailableCapacity"/>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Bom_Product_1 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Bom_Product_1">
        <rdf:type rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#BillOfMaterial"/>
        <Planning-Ontology:hasLocation rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Frontend_1"/>
        <Planning-Ontology:hasProduct rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Product_1"/>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Borderless_FE1_zu_FE2 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Borderless_FE1_zu_FE2">
        <rdf:type rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningObject"/>
        <Planning-Ontology:hasActivities rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Activity_Photo"/>
        <Planning-Ontology:hasLocation rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Frontend_2"/>
        <Planning-Ontology:hasOwner rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#FE1"/>
        <Planning-Ontology:hasProduct rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Product_1"/>
        <Planning-Ontology:hasDueDate>1 week</Planning-Ontology:hasDueDate>
        <Planning-Ontology:hasQuantity rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</Planning-Ontology:hasQuantity>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Capacity_Restriction_Equation -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Capacity_Restriction_Equation">
        <rdf:type rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningConstraint"/>
        <sc3:formula>forall($k in WorkCenters, $j in Frontends, t in PlanningHorizon) sum($g in Products, $l in OperationCountOnWorkCenter($j, $k, $g) -&gt;
		FE_Y($j, $g, $t, OperationsOnWorkCenter($j, $k, $g, $l)) * alpha($j, $g, OperationsOnWorkCenter($j, $k, $g, $l)) &lt;= WorkCenterCapacity($t, $j, $k);
	

WorkCenters (Resources)=FE_Aligner, FE_Stepper_2, FE_Stepper_1, FE_Implanter
Frontends= Frontend_1, Frontend_2;
Products= Product_1;
OperationCountOnWorkCenter=Product($numberOfOperationsOnSameResource);
alpha= Activity($hasRequiredCapacity(Product($hasActivities)));
WorkCenterCapacity= FE_Aligner($hasAvailableCapacity, $hasLocation), FE_Stepper_2($hasAvailableCapacity, $hasLocation), FE_Stepper_1($hasAvailableCapacity, $hasLocation), FE_Implanter($hasAvailableCapacity, $hasLocation);</sc3:formula>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#FE1 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#FE1">
        <rdf:type rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Owner"/>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#FE1_CostMinimization -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#FE1_CostMinimization">
        <rdf:type rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlaningPurpose"/>
        <Planning-Ontology:hasCriterias rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Minimization"/>
        <Planning-Ontology:hasPerformanceMeasures rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#MinimizeBacklog"/>
        <Planning-Ontology:hasPerformanceMeasures rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#MinimizeInventoryLevel"/>
        <Planning-Ontology:hasPerformanceMeasures rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#MinimizeWipCosts"/>
        <Planning-Ontology:hasPlanningObjects rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#FE_Backlog"/>
        <Planning-Ontology:hasPlanningObjects rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#FE_Inventory"/>
        <Planning-Ontology:hasPlanningObjects rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#FE_Wip"/>
        <Planning-Ontology:hasPlanningObjects rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#FE_X"/>
        <Planning-Ontology:hasPlanningObjects rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#FE_Y"/>
        <Planning-Ontology:hasCosts>backlog=150</Planning-Ontology:hasCosts>
        <Planning-Ontology:hasCosts>inventory=15</Planning-Ontology:hasCosts>
        <Planning-Ontology:hasCosts>wip=50</Planning-Ontology:hasCosts>
        <Planning-Ontology:hasPlanningWindow>12 weeks</Planning-Ontology:hasPlanningWindow>
        <sc3:formula>sum($g in Products, $t in PlanningHorizon)--&gt; sum($j in Frontends) (WipCosts($g, $t) * FE_Wip($j, $g, $t)) + InventoryCosts($g, $t) * FE_Inventory($g, $t) + BacklogCosts($g, $t) * FE_Backlog($g, $t);

Frontends= Frontend_1, Frontend_2;
Products= Product_1;
InventoryCosts= $(hasCosts($inventory));
BacklogCosts= $(hasCosts($backlog));
WipCosts= $(hasCosts($wip));</sc3:formula>
