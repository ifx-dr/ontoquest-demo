        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#DecisionMakingUnit"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>This property describes the point of time when a decision is made by the DECISION-MAKING UNIT.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasDemandClass -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasDemandClass">
