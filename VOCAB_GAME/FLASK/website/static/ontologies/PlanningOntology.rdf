        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningConstraint"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>This is the latest point in time when the PLANNING CONSTRAINT is active.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasFeasibilityStatus -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasFeasibilityStatus">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#Plan"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>A PLAN is feasible if all PLANNING CONSTAINTS of the PLANNING INSTANCE on which the plan is based are fulfilled, otherwise, the plan is infeasible.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasHardConstraints -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasHardConstraints">
