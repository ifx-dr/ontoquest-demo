        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningConstraint"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>PLANNING CONSTRAINTS with this property cannot be violated.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasIdentifier -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasIdentifier">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#PlanningObject"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Each PLANNING OBJECT has a unique identifier, abbreviated by ID.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasInformationStatus -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/sc3/ontologies/2022/0/Planning-Ontology#hasInformationStatus">
